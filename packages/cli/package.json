{
  "name": "@vslides/cli",
  "version": "0.0.1",
  "type": "module",
  "license": "MIT",
  "main": "dist/cli.mjs",
  "types": "dist/cli.d.ts",
  "files": [
    "bin/",
    "dist/"
  ],
  "bin": {
    "vslides": "bin/vslides.mjs"
  },
  "scripts": {
    "build": "rimraf dist && unbuild"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^3.2.0",
    "@vslides/core": "workspace:^",
    "@vslides/vite-plugin": "workspace:^",
    "cac": "^6.7.14",
    "unocss": "^0.47.4",
    "vite": "^3.2.4",
    "vue": "^3.2.45"
  },
  "unbuild": {
    "entries": [
      "src/cli"
    ],
    "declaration": true,
    "externals": [
      "vite",
      "@vslides/vite-plugin"
    ]
  }
}
